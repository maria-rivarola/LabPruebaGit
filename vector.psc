Algoritmo vector
	Definir vec, long Como Entero
	long <- 4
	Dimension vec[long]
	
	//CARGAR VECTOR
	Para i<-0 Hasta long - 1 Con Paso 1 Hacer
		vec[i] <- Aleatorio(10,15)
	Fin Para
	
	//MOSTRAR VECTOR
	Para i<-0 Hasta long - 1 Con Paso 1 Hacer
		Escribir Sin Saltar vec[i], " "
	Fin Para
	
FinAlgoritmo
